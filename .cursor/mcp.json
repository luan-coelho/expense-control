      "command": "npx",
      "args": ["-y", "--package=task-master-ai", "task-master-ai"]
    },
    "server-sequential-thinking": {
      "command": "npx",
      "args": ["-y", "@smithery/cli@latest", "run", "@smithery-ai/server-sequential-thinking"]
    },
    "github": {
      "type": "http",
